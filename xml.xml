<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
<PREFIX>
  <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
  <LABEL>owl</LABEL>
</PREFIX>
<SOURCE>
  <ID>sourceId</ID>
  <ENDPOINT></ENDPOINT>
  <VAR>?s</VAR>
  <PAGESIZE>1000</PAGESIZE>
  <RESTRICTION>?s rdf:type some:Type</RESTRICTION>
  <PROPERTY>dc:title AS lowercase RENAME name</PROPERTY>


  <TYPE>sparql</TYPE>
</SOURCE>
<TARGET>
  <ID>targetId</ID>
  <ENDPOINT></ENDPOINT>
  <VAR>?t</VAR>
  <PAGESIZE>1000</PAGESIZE>
  <RESTRICTION>?t rdf:type some:Type</RESTRICTION>
  <PROPERTY>foaf:name AS lowercase RENAME name</PROPERTY>


  <TYPE>sparql</TYPE>
</TARGET>
<METRIC>
  
</METRIC>
<ACCEPTANCE>
  <THRESHOLD>0.98</THRESHOLD>
  <FILE>accepted.nt</FILE>
  <RELATION>owl:sameAs</RELATION>
</ACCEPTANCE>
<REVIEW>
  <THRESHOLD>0.9</THRESHOLD>
  <FILE>reviewme.nt</FILE>
  <RELATION>owl:sameAs</RELATION>
</REVIEW>
<EXECUTION>
  <REWRITER>DEFAULT</REWRITER>
  <PLANNER>DEFAULT</PLANNER>
  <ENGINE>DEFAULT</ENGINE>
</EXECUTION>
<OUTPUT>TAB</OUTPUT>
</LIMES>